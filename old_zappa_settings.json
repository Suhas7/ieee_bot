{
    "prod": {
      "app_function": "ieee-bot.app",
      "aws_region": "us-east-2",
      "exclude": [
        "__pycache__",
        ".git/*",
        ".gitignore",
        ".python-version",
        "LICENSE",
        "README.md",
        "requirements.txt",
        "zappa_settings.json",
        "client_secret.json",
        "venv"
      ],
      "keep_warm": true,
      "keep_warm_expression": "rate(5 minutes)",
      "memory_size": 128,
      "profile_name": "default",
      "project_name": "ieee-bot",
      "runtime": "python3.8",
      "s3_bucket": "ieee-bot",
      "timeout_seconds": 30,
    }
  }